{
  "extends": ["tslint:recommended"],
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-last"
      }
    ]
  },
  "exclude": ["node_modules", "dist", "build", ".next"]
}
